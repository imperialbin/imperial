<% switch (true) {
    case deleteDate == 'Deletes after being viewed.' : %>
        <% if( loggedIn ) {%>
            <%- include('./menus/loggedInDelete.ejs'); %>
        <% } else { %>
            <%- include('./menus/loggedOut.ejs'); %>
        <% }%>
        <% break;
    
    case loggedIn && creator : %>
        <%- include('./menus/loggedInCreator.ejs'); %>
        <% break;
    
    case loggedIn : %>
        <%- include('./menus/loggedIn.ejs'); %>
        <% break;
    
    default: %>
        <%- include('./menus/loggedOut.ejs'); %>
        <% break;
} %>